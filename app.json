{
  "expo": {
    "name": "Be-Okay",
    "slug": "Be-Okay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/appicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#8BB85C"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appicon.png",
        "backgroundColor": "#8BB85C"
      },
      "package": "com.muhayimana_ambroise123.BeOkay"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "30ba0648-7240-4f86-9f5f-e7bf577e7877"
      }
    }
  }
}
